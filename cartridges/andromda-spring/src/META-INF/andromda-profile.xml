<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The default Spring profile mappings.
-->
<mappings name="spring">
    <mapping>
        <from>HIBERNATE_QUERY</from>
        <to>@andromda.hibernate.query</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_INHERITANCE</from>
        <to>@andromda.hibernate.inheritance</to>
    </mapping>
    <mapping>
        <from>SPRING_SERVICE_REMOTING_TYPE</from>
        <to>@andromda.spring.service.remoting.type</to>
    </mapping>
    <mapping>
        <from>SPRING_SERVICE_REMOTE_PORT</from>
        <to>@andromda.spring.service.remote.port</to>
    </mapping>
</mappings>