<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The default Database profile mappings.
-->
<mappings name="database">
    <mapping>
        <from>DATABASE_DUMMYLOAD_SIZE</from>
        <to>@andromda.persistence.dummyload.size</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_DISPLAYNAME</from>
        <to>@andromda.persistence.console.table.displayname</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_DISPLAYCOLUMN</from>
        <to>@andromda.persistence.console.table.displaycolumn</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_PAGESIZE</from>
        <to>@andromda.persistence.console.table.pagesize</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_MAXLISTSIZE</from>
        <to>@andromda.persistence.console.table.maxlistsize</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_EXPORTABLE</from>
        <to>@andromda.persistence.console.table.exportable</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_TABLE_INSERTABLE</from>
        <to>@andromda.persistence.console.table.insertable</to>
    </mapping>


    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_DISPLAYNAME</from>
        <to>@andromda.persistence.console.column.displayname</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_RESOLVEFK</from>
        <to>@andromda.persistence.console.column.resolvefk</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_HIDE</from>
        <to>@andromda.persistence.console.column.hide</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_SORTABLE</from>
        <to>@andromda.persistence.console.column.sortable</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_EXPORTABLE</from>
        <to>@andromda.persistence.console.column.exportable</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_UPDATEABLE</from>
        <to>@andromda.persistence.console.column.updateable</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_SIZE</from>
        <to>@andromda.persistence.console.column.size</to>
    </mapping>
    <mapping>
        <from>DATABASE_CONSOLE_COLUMN_VALUES</from>
        <to>@andromda.persistence.console.column.values</to>
    </mapping>


</mappings>