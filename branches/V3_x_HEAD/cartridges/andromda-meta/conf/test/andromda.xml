<andromda>
    <properties>
        <property name="failOnValidationErrors">false</property>
    </properties>
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${test.model.uri}</uri>
                    <moduleSearchLocations>
                        <location patterns="**/*.xml.zip">${basedir}/../../profiles/uml-1.4</location>
                        <location patterns="**/*.xml.zip">${pom.basedir}/../../profiles/uml-1.4</location>
                        <location patterns="**/*.xml">${basedir}/../../profiles/uml-1.4</location>
                        <location patterns="**/*.xml.zip">${basedir}/../../metafacades/uml/common</location>
                        <location patterns="**/*.xml.zip">${pom.basedir}/../../metafacades/uml/common</location>
                        <location patterns="**/3.4-SNAPSHOT/*-3.4-SNAPSHOT.xml.zip">${settings.localRepository}/org/andromda/profiles/uml14</location>
                        <location patterns="**/3.4-SNAPSHOT/*-3.4-SNAPSHOT.xml.zip">${settings.localRepository}/org/andromda/metafacades</location>
                    </moduleSearchLocations>
                    <modelPackages>
                        <modelPackage process="false">org::andromda::metafacades::uml</modelPackage>
                    </modelPackages>
                </model>
            </models>
        </repository>
    </repositories>
    <namespaces>
        <namespace name="meta">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="facades">${test.output.dir}</property>
                <property name="facade-logics">${test.output.dir}</property>
                <property name="facades-to-impls">${test.output.dir}</property>
                <property name="facade-logic-impls">${test.output.dir}</property>
                <property name="overwrite">true</property>
                <property name="enableTemplating">false</property>
                <property name="typeSafeEnumsEnabled">false</property>
                <property name="pluralizeAssociationEndNames">true</property>
                <property name="pluralizeAttributeNames">false</property>
                <property name="pluralizeParameterNames">false</property>
                <property name="toDoTag">@todo</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>