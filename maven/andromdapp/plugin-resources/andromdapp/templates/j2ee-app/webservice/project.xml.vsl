<project>
    <extend>../project.xml</extend>
    <name>${projectName} WebService</name>
    <artifactId>${pom.groupId}-webservice</artifactId>
    <shortDescription>${pom.name} WebService Component</shortDescription>
    <dependencies>
        <dependency>
            <groupId>${pom.groupId}</groupId>
            <artifactId>${pom.groupId}-core</artifactId>
            <version>${pom.currentVersion}</version>
            <type>ejb</type>
        </dependency>
        <dependency>
            <groupId>axis</groupId>
            <artifactId>axis</artifactId>
            <version>1.2-RC3</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
