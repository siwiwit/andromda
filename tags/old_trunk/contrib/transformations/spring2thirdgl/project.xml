<?xml version="1.0" encoding="UTF-8"?>

<project>
	<extend>../project.xml</extend>
	<artifactId>andromda-transformations-spring2thirdgl</artifactId>
	<currentVersion>1.0-SNAPSHOT</currentVersion>
	<name>AndroMDA Spring2ThirdGL Transformation</name>
	<description> The AndroMDA Spring2ThirdGL transformation.
	</description>
	<dependencies>
		<dependency>
			<groupId>${pom.groupId}</groupId>
			<artifactId>${pom.groupId}-metamodels-spring</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>${pom.groupId}</groupId>
			<artifactId>${pom.groupId}-metamodels-3gl</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
	</dependencies>
	<build>
		<unitTest>
			<resources>
				<resource>
					<directory>src/test/java</directory>
					<includes>
						<include>**/*.xml</include>
					</includes>
					<filtering>false</filtering>
				</resource>
				<resource>
					<directory>src/test/spring</directory>
					<includes>
						<include>**/*.xmi</include>
					</includes>
					<filtering>false</filtering>
				</resource>
			</resources>
		</unitTest>
	</build>
</project>
