<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="androidnature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="org.andromda.android.core.project.AndroidNature"/>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.andromda.android.core.internal.settings.PreferenceInitializer"/>
   </extension>
   <extension point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            file-names="andromda.xml"
            id="andromdaconfiguration"
            name="AndroMDA Configuration"
            priority="high">
         <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
            <parameter name="element" value="andromda"/>
         </describer>
      </content-type>
   </extension>
</plugin>
