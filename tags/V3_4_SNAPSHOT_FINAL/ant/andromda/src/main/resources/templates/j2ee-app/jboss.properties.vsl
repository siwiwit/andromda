# enter the local jboss installation path here
jboss.installation.path=C:/apps/jboss-3.2.6

# enter the path to the deploy directory
jboss.default.deploy.dir=server/default/deploy

# enter the URI to the remote server
# this could also be a windows-like local system path
jboss.remote.path=//192.168.1.9

# enter the remote path or windows share
jboss.remote.deploy.dir=deploy
