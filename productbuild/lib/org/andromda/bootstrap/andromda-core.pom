        </includes>
      </resource>
      <resource>
        <filtering>true</filtering>
        <directory>f:\java\development\andromda-all\andromda\src\main\resources</directory>
        <includes>
          <include>META-INF/**/*</include>
        </includes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>f:\java\development\andromda-all\andromda\src\test\java</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </testResource>
    </testResources>
    <directory>f:\java\development\andromda-all\andromda\target</directory>
    <finalName>andromda-core-3.1-SNAPSHOT</finalName>
    <plugins>
      <plugin>
        <groupId>org.andromda.maven.plugins</groupId>
        <artifactId>andromda-bootstrap-install-plugin</artifactId>
        <version>3.1-SNAPSHOT</version>
        <executions>
          <execution>
            <goals>
              <goal>install</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <installDirectory>f:\java\development\andromda-all\andromda/../lib</installDirectory>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <version>2.0-rc2-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <version>2.0-beta-3-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.0-beta-2-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.0-beta-2-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.0-rc2-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-install-plugin</artifactId>
        <version>2.0-beta-2-SNAPSHOT</version>
