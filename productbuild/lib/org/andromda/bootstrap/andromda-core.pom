    <testSourceDirectory>f:\java\development\andromda-all\andromda\src\test\java</testSourceDirectory>
    <outputDirectory>f:\java\development\andromda-all\andromda\target\classes</outputDirectory>
    <testOutputDirectory>f:\java\development\andromda-all\andromda\target\test-classes</testOutputDirectory>
