    <mapping>
        <from>ENTITY</from>
        <to>Entity</to>
    </mapping>
    <mapping>
        <from>SERVICE</from>
        <to>Service</to>
    </mapping>
    <mapping>
        <from>FINDER_METHOD</from>
        <to>FinderMethod</to>
    </mapping>
    <mapping>
        <from>IDENTIFIER</from>
        <to>PrimaryKey</to>
