    <mapping>
        <from>UNIQUE</from>
        <to>Unique</to>
    </mapping>
    <mapping>
        <from>VALUE_OBJECT</from>
        <to>ValueObject</to>
    </mapping>
    <mapping>
        <from>EXCEPTION</from>
        <to>Exception</to>
    </mapping>
    <mapping>
        <from>APPLICATION_EXCEPTION</from>
        <to>ApplicationException</to>
    </mapping>
    <mapping>
        <from>UNEXPECTED_EXCEPTION</from>
        <to>UnexpectedException</to>
    </mapping>
    <mapping>
