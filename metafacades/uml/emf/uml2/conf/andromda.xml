<andromda xmlns="http://andromda.org/core/configuration">
    <properties>
        <property name="cartridgeFilter">meta</property>
    </properties>
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${maven.andromda.model.uri}</uri>
                    <moduleSearchLocations>
                        <location patterns="**/*.xml.zip">${pom.basedir}/src/uml</location>
                        <location patterns="**/*.xml.zip">${pom.basedir}/../../common/src/uml</location>
                        <location patterns="**/*.xml.zip">${pom.basedir}/../../../../profiles/uml-1.4/</location>
                        <location patterns="**/*.xml.zip">${settings.localRepository}/org/andromda/profiles/uml14</location>
                        <location patterns="**/3.4-SNAPSHOT/*-3.4-SNAPSHOT.xml.zip">${settings.localRepository}/org/andromda/profiles/uml14</location>
                        <location patterns="**/3.4-SNAPSHOT/*-3.4-SNAPSHOT.xml.zip">${settings.localRepository}/org/andromda/metafacades</location>
                        <location patterns="**/*.xml">jar:file:${settings.localRepository}/org/omg/profiles/uml14/UML_Standard_Profile/1.0/UML_Standard_Profile-1.0.jar!/</location>
                    </moduleSearchLocations>
                </model>
            </models>            
        </repository>
    </repositories>
    <namespaces>
        <namespace name="meta">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="facades" ignore="true"/>
                <property name="facade-logics">${maven.build.src}</property>
                <property name="facade-logic-impls">${maven.src.dir}/java</property>
                <property name="metamodelVersionPackage">org.andromda.metafacades.emf.uml2</property>            
                <property name="enableTemplating">true</property>
                <property name="enableAnnotations">true</property>
                <property name="typeSafeEnumsEnabled">true</property>
                <property name="enumerationLiteralNameMask">upperunderscore</property>
                <property name="pluralizeAssociationEndNames">true</property>
                <property name="pluralizeAttributeNames">true</property>
                <property name="pluralizeParameterNames">true</property>
                <property name="toDoTag">TODO</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>
