<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>../../../project.xml</extend>
    <artifactId>andromda-taglibs</artifactId>
    <name>AndroMDA Web Taglibs</name>
    <shortDescription>Contains taglibs for AndroMDA generated applications</shortDescription>
    <description>
        This modules contains tag-libaries that can be used by applications generated using AndroMDA. These libraries
        have been developed because no suitable libraries could be found in other open-source projects.
    </description>
    <repository>
        <connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/andromda:andromda/etc/web/taglibs</connection>
        <url>http://cvs.sourceforge.net/viewcvs.py/andromda/etc/web/taglibs/</url>
    </repository>
    <dependencies>
        <dependency>
            <groupId>commons-beanutils</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.7.0</version>
        </dependency>
        <dependency>
            <groupId>commons-collections</groupId>
            <artifactId>commons-collections</artifactId>
            <version>3.1</version>
        </dependency>
        <dependency>
            <groupId>commons-digester</groupId>
            <artifactId>commons-digester</artifactId>
            <version>1.5</version>
        </dependency>
        <dependency>
            <groupId>commons-lang</groupId>
            <artifactId>commons-lang</artifactId>
            <version>2.0</version>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.0.4</version>
        </dependency>
        <dependency>
            <groupId>servletapi</groupId>
            <artifactId>servletapi</artifactId>
            <version>2.4-20040521</version>
        </dependency>
        <!-- taglib dependencies -->
        <dependency>
            <groupId>tomcat</groupId>
            <artifactId>jsp-api</artifactId>
            <version>5.0.18</version>
        </dependency>
    </dependencies>
</project>
