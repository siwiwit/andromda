            <context>org.andromda.metafacades.uml.Entity</context>
        </mapping>
        <property reference="sqlMappingsUri">
            <documentation>
                <![CDATA[
                The URI to the the SQL mappings file.
                ]]>
            </documentation>
        </property>
        <property reference="jdbcMappingsUri">
            <documentation>
