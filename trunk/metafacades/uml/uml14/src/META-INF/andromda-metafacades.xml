        <mapping class="org.omg.uml.foundation.core.UmlClass$Impl">
            <stereotype>ENTITY</stereotype>
        </mapping>
        <property reference="allowDefaultIdentifiers" default="true"/>
        <property reference="defaultIdentifierPattern" default="id"/>
        <property reference="defaultIdentifierType" default="datatype::Long"/>
        <property reference="defaultIdentifierVisibility" default="public"/>
        <property reference="relationNameSeparator" default="2"/>
    </metafacade>
    <metafacade class="org.andromda.metafacades.uml14.EntityAttributeLogicImpl">
