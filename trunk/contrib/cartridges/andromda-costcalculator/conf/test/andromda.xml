<andromda>
    <properties>
        <property name="failOnValidationErrors">false</property>
		<!--
        <property name="loggingConfigurationUri">file:${basedir}/conf/test/log4j.xml</property>
		-->
    </properties>
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${andromda.cartridge.test.model.uri}</uri>
                    <moduleSearchLocations>
                        <location>${maven.repo.local}/andromda/xml.zips</location>
                    </moduleSearchLocations>
                    <modelPackages processAll="false">
                        <modelPackage process="true">org::andromda::cartridges::costcalculator::test</modelPackage>
                    </modelPackages>
                </model>
            </models>
        </repository>
    </repositories>
    <namespaces>
        <namespace name="costcalculator">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="jdbcMappingsUri">JDBC</property>
                <property name="spreadsheet">${test.output.dir}</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>
