<project default="uml2mof"
    xmlns:artifact="artifact"
    xmlns:j="jelly:core"
    xmlns:ant="jelly:ant"
    xmlns:velocity="jelly:velocity">

    <goal name="uml2mof" prereqs="clean">
        <j:set var="uml.output.dir" value="target/uml"/>
        <ant:unzip src="${model.file}" dest="${uml.output.dir}"/>
        <j:set var="andromda.uml2mof.model.file" value="${uml.output.dir}/3GLMetamodel.xml"/>
        <attainGoal name="uml2mof:run"/>
    </goal>

    <goal name="clean">
        <attainGoal name="uml2mof:clean"/>
    </goal>

</project>