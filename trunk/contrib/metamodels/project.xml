<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>../project.xml</extend>
    <artifactId>andromda-metamodels</artifactId>
    <currentVersion>1.0-SNAPSHOT</currentVersion>
    <name>AndroMDA Metamodels Spring</name>
    <description>
        Contains the metamodel for Spring applications.
    </description>
    <dependencies>
        <dependency>
            <groupId>${pom.groupId}</groupId>
            <artifactId>${pom.groupId}-uml2mof-plugin</artifactId>
            <version>1.0-SNAPSHOT</version>
            <type>plugin</type>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <resources>
            <resource>
                <directory>${maven.build.dir}/xmi</directory>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
            </resource>
        </resources>
    </build>
</project>
