<andromda>
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${andromda.translationlibrary.model.uri}</uri>
                    <moduleSearchLocations>
                        <location patterns="**/src/main/uml/*.xml.zip">${project.basedir}/../../../profiles/uml-1.4
                        </location>
                        <location patterns="**/*.xml">
                            jar:file:${settings.localRepository}/org/omg/profiles/uml14/UML_Standard_Profile/1.0/UML_Standard_Profile-1.0.jar!/
                        </location>
                    </moduleSearchLocations>
                </model>
            </models>
        </repository>
    </repositories>
    <namespaces>
        <namespace name="default">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>