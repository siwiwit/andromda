<?xml version="1.0" encoding="UTF-8"?>

<document xmlns="http://maven.apache.org/XDOC/2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://maven.apache.org/XDOC/2.0 http://maven.apache.org/xsd/xdoc-2.0.xsd">
    <properties>
        <title>Query Translation-Library</title>
    </properties>
    <body>
        <section name="Query Translation-Library">
            <p>
                Performs translation of OCL query expressions to different query
                languages.
            </p>
            <p>Currently Supported Translations:</p>
            <ul>
                <li>
                    <a href="http://java.sun.com/j2ee/1.4/docs/tutorial/doc/EJBQL.html">Enterprise Java Bean Query
                        Language
                    </a>
                </li>
                <li>
                    <a href="http://www.hibernate.org/hib_docs/reference/en/html/queryhql.html">Hibernate Query
                        Language
                    </a>
                </li>
            </ul>
        </section>
    </body>
</document>
