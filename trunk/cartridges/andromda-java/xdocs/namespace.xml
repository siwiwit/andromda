<?xml version="1.0" encoding="UTF-8"?>

<document>
  	<properties>
    	<title>Java Cartridge Namespace</title>
  	</properties>
  	<body>
    	<section name="Java Cartridge Namespace Properties">
	      	<table>
	        	<tr>
		          	<th>Property</th>         	
		          	<th>Description</th>       	
		          	<th>Required?</th>	   	 
	        	</tr>
	        	<tr>
		          	<td>
		          		languageMappingsUri
		          	</td>
		          	<td>
		          		URI specifying the specific mappings from model types to language types. 
		          		(i.e. <code>file:${basedir}/JavaMappings.xml</code>)
		          	</td>
		          	<td>
		          		Yes
		          	</td>		          	
	        	</tr>	
	        	<tr>
		          	<td>
		          		services
		          	</td>
		          	<td>
		          		The directory to which POJO service interfaces
						are generated.
		          	</td>
		          	<td>
		          		No
		          	</td>
	        	</tr>	
	        	<tr>
		          	<td>
		          		service-impls
		          	</td>
		          	<td>
		          		The directory to which POJO service implementation 
						classes are generated.
		          	</td>
		          	<td>
		          		No
		          	</td>
	        	</tr>	
	        	<tr>
		          	<td>
		          		value-objects
		          	</td>
		          	<td>
		          		The directory to which value objects are generated.
		          	</td>
		          	<td>
		          		No
		          	</td>
	        	</tr>	
	        	<tr>
		          	<td>
		          		exceptions
		          	</td>
		          	<td>
		          		The directory to which exceptions are generated.
		          	</td>
		          	<td>
		          		No
		          	</td>
	        	</tr>		  
	        	<tr>
		          	<td>
		          		enumerations
		          	</td>
		          	<td>
		          		The directory to which enumerations are generated. 
		          	</td>
		          	<td>
		          		No
		          	</td>		          	
	        	</tr>			        	   	      	        	        		        	        	        	     	             	        	
	        </table>
    	</section>
  	</body>
</document>