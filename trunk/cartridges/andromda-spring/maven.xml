<project default="jar:install"
         xmlns:ant="jelly:ant">
            
    <!-- ==================================================================
             Cleans the cartridge
         ================================================================== -->        
    <goal name="clean">
        <attainGoal name="clean:clean"/>
    </goal>   
         
    <!-- Unzip the howto images -->
    <preGoal name="xdoc:transform">
        <ant:unzip src="${howto.pictures.zip}" dest="${howto.output.dir}"/>
    </preGoal>
    
</project>

