<?xml version="1.0" encoding="ISO-8859-1" ?>

<profile>
    <elements>
        <elementGroup name="Stereotypes">
            <element name="METACLASS">
                <documentation>
                    From standard UML, used by this cartridge to associate
                    a metafacade to the meta model element it shields.
                </documentation>
                <value>metaclass</value>
                <appliedOnElement>class</appliedOnElement>
            </element>
            <element name="METAFACADE">
                <documentation>
                    Denotes an AndroMDA meta-model facade.
                </documentation>
                <value>metafacade</value>
                <appliedOnElement>class</appliedOnElement>
            </element>
            <element name="PSM_METACLASS">
                <documentation>
                    Denotes a class that represents a metaclass
					in a PSM metamodel.
                </documentation>
                <value>PSMmetaclass</value>
                <appliedOnElement>class</appliedOnElement>
            </element>
        </elementGroup>
        <elementGroup name="Tagged Values">
            <element name="GENERALIZATION_PRECEDENCE">
                <documentation>
                    Allows the specification of precedence when a metafacade generalizes
                    from more than one metafacade. Takes an integer as it's value (i.e. 1, 2, 3, etc)
                    where one denotes the highest precedence.
                </documentation>
                <value>andromda_meta_generalization_precedence</value>
                <appliedOnElement>A generalization between metafacades.</appliedOnElement>
                <allowedValues>
                    <value>a strictly positive integer</value>
                </allowedValues>
            </element>
        </elementGroup>
    </elements>
</profile>