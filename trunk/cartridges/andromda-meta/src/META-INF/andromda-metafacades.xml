<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- contains andromda-meta's metafacade mappings -->
<metafacades namespace="meta">
    <property reference="implementationOperationNamePattern"/>
    <metafacade class="org.andromda.cartridges.meta.metafacades.MetafacadeLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.UmlClass$Impl">
            <stereotype>METAFACADE</stereotype>
        </mapping>
        <property reference="metamodelVersionPackage"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.meta.metafacades.MetafacadeOperationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Operation$Impl">
            <context>org.andromda.cartridges.meta.metafacades.Metafacade</context>
        </mapping>
    </metafacade>
    <metafacade class="org.andromda.cartridges.meta.metafacades.MetafacadeAssociationEndLogicImpl">
        <mapping class="org.omg.uml.foundation.core.AssociationEnd$Impl">
            <context>org.andromda.cartridges.meta.metafacades.Metafacade</context>
        </mapping>
    </metafacade>
    <metafacade class="org.andromda.cartridges.meta.metafacades.MetafacadeAttributeLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Attribute$Impl">
            <context>org.andromda.cartridges.meta.metafacades.Metafacade</context>
        </mapping>
    </metafacade>
    <metafacade class="org.andromda.cartridges.meta.metafacades.MetafacadeGeneralizationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Generalization$Impl">
            <context>org.andromda.cartridges.meta.metafacades.Metafacade</context>
        </mapping>
        <property reference="generalizationNamePattern"/>
    </metafacade>
</metafacades>
