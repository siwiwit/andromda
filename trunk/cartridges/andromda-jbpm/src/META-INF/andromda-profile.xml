<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The default jBPM profile mappings.
-->
<mappings name="jbpm">
    <!-- stereotypes -->
    <mapping>
        <from>BUSINESS_PROCESS</from>
        <to>Business Process</to>
    </mapping>
    <mapping>
        <from>BEFORE_SIGNAL</from>
        <to>Before Signal</to>
    </mapping>
    <mapping>
        <from>AFTER_SIGNAL</from>
        <to>After Signal</to>
    </mapping>
    <mapping>
        <from>NODE_ENTER</from>
        <to>Node Enter</to>
    </mapping>
    <mapping>
        <from>NODE_LEAVE</from>
        <to>Node Leave</to>
    </mapping>
    <mapping>
        <from>TASK</from>
        <to>Task</to>
    </mapping>
    <!-- tagged values -->
    <mapping>
        <from>ASSIGNMENT_EXPRESSION</from>
        <to>@andromda.process.jbpm.assignment.expression</to>
    </mapping>
    <!-- datatype mappings -->
</mappings>