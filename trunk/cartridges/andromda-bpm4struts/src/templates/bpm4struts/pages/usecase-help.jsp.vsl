            <bean:message key="$useCase.onlineHelpKey"/>
        </p>
#foreach ($jsp in $useCase.pages)
        <h2>
            <html:link page="${jsp.fullPath}_help.jsp"><bean:message key="$jsp.titleKey"/></html:link>
	    </h2>
        <p>
            <bean:message key="$jsp.onlineHelpKey"/>
        </p>
#end
        <html:link page="${pagePathPrefix}/application-help.jsp"><bean:message key="online.help.main"/></a>
    </tiles:put>

</tiles:insert>
#end
