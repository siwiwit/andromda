<andromdapp overwrite="true">
    <properties>
        <property name="author">AndroMDA Team</property>
        <property name="applicationParentDirectory">$${project.basedir}</property>
        <property name="applicationId">car-rental-system</property>
        <property name="applicationName">AndroMDA Car Rental Sample</property>
        <property name="applicationVersion">$${version}</property>
        <property name="applicationPackage">org.andromda.samples.carrental</property>
        <property name="applicationType">ear</property>
        <property name="transactionPersistenceType">spring</property>
        <property name="workflow">no</property>
        <property name="hibernateVersion">2</property>
        <property name="web">yes</property>
        <property name="webType">struts</property>
        <property name="webservice">no</property>
        <property name="model">jar:file:${project.build.sourceDirectory}/CarRentalSystem.xml.zip!/CarRentalSystem.xml</property>
        <property name="configurationUri">${project.basedir}/conf/andromda.xml</property>
    </properties>
</andromdapp>