<?xml version="1.0" encoding="UTF-8"?>
<andromdapp overwrite="true">
    <properties>
        <property name="andromdappType">j2ee</property>
        <property name="author">AndroMDA Team</property>
        <property name="modelType">uml1.4</property>
        <property name="applicationParentDirectory">$${project.basedir}</property>
        <property name="applicationId">online-store</property>
        <property name="applicationName">AndroMDA Online Store Sample</property>
        <property name="applicationVersion">$${project.version}</property>
        <property name="applicationPackage">org.andromda.samples.onlinestore</property>
        <property name="applicationType">war</property>
        <property name="transactionPersistenceType">none</property>
        <property name="web">yes</property>
        <property name="webservice">none</property>
        <property name="model">jar:file:${project.basedir}/src/main/uml/online-store.xml.zip!/online-store.xml</property>
        <property name="databaseType">h2</property>
        <property name="testDatabase">h2</property>
        <property name="deploymentPlatform">jboss7</property>
        <property name="configurationUri">${project.basedir}/src/main/config/andromda.xml</property>
        <!--property name="distributionManagementRepositoryUrl">$${project.distributionManagement.repository.url}</property>
        <property name="distributionManagementSnapshotRepositoryUrl">$${project.distributionManagement.snapshotRepository.url}</property-->
        <property name="distributionManagementRepositoryUrl">file:/scratch/hudson/workspace/Andromda/</property>
        <property name="distributionManagementSnapshotRepositoryUrl">file:/scratch/hudson/workspace/Andromda/</property>
        <property name="distributionManagementRepositoryId">$${project.distributionManagement.repository.id}</property>
        <property name="distributionManagementSnapshotRepositoryId">$${project.distributionManagement.snapshotRepository.id}</property>
        <property name="inceptionYear">2003</property>
        <property name="projectUrl">http://www.andromda.org/</property>
        <property name="issueSystem">jira</property>
        <property name="issueUrl">http://jira.andromda.org/</property>
        <property name="scmConnection">scm:svn:http://andromda.svn.sourceforge.net/svnroot/andromda/trunk/samples/online-store</property>
        <property name="organizationName">AndroMDA.org</property>
        <property name="organizationUrl">http://www.andromda.org/</property>
        <property name="licenseName">BSD</property>
        <property name="licenseUrl">http://www.andromda.org/docs/license.html</property>
        <property name="ciSystem">jenkins</property>
        <property name="ciUrl">https://andromdabuild.ci.cloudbees.com/job/Andromda/</property>
        <property name="website">file:/scratch/hudson/workspace/Andromda/site/online-store/</property>
    </properties>
</andromdapp>