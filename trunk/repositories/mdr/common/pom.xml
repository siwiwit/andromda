<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
         xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.andromda.repositories</groupId>
        <artifactId>andromda-repositories</artifactId>
        <version>3.4-SNAPSHOT</version>
    </parent>
    <artifactId>andromda-repository-mdr</artifactId>
    <packaging>jar</packaging>
    <name>AndroMDA MDR Repository (Common)</name>
    <description>
        This MDR repository module allows AndroMDA to use the NetBeans MDR as a repository for loading,
        and querying object models.
    </description>
    <scm>
        <connection>scm:svn:http://andromda.svn.sourceforge.net/svnroot/andromda/trunk/repositories/mdr/common</connection>
        <developerConnection>scm:svn:https://andromda.svn.sourceforge.net/svnroot/andromda/trunk/repositories/mdr/common</developerConnection>
        <url>http://andromda.svn.sourceforge.net/</url>
    </scm>
    <dependencies>
        <dependency>
            <groupId>org.andromda</groupId>
            <artifactId>andromda-core</artifactId>
            <version>${project.version}</version>
        </dependency>

        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
        </dependency>
        <dependency>
            <groupId>commons-lang</groupId>
            <artifactId>commons-lang</artifactId>
        </dependency>

        <dependency>
            <groupId>org.andromda.thirdparty.netbeans.mdr</groupId>
            <artifactId>jmiutils</artifactId>
        </dependency>
        <dependency>
            <groupId>org.andromda.thirdparty.netbeans.mdr</groupId>
            <artifactId>mdrapi</artifactId>
        </dependency>
        <dependency>
            <groupId>org.andromda.thirdparty.netbeans.mdr</groupId>
            <artifactId>nbmdr</artifactId>
        </dependency>
        <dependency>
            <groupId>org.andromda.thirdparty.netbeans.mdr</groupId>
            <artifactId>openide-util</artifactId>
        </dependency>
        <dependency>
            <groupId>org.andromda.thirdparty.jmi</groupId>
            <artifactId>mof</artifactId>
        </dependency>
        <dependency>
            <groupId>org.andromda.thirdparty.jmi</groupId>
            <artifactId>jmi</artifactId>
        </dependency>
    </dependencies>
</project>
